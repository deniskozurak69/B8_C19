@model LibraryWebApplication1.Models.ApplicationUser

<h2>Register</h2>

<form asp-action="Register" method="post">
    <div>
        <label>Email</label>
        <input asp-for="Email" type="email" />
    </div>
    <div>
        <label>Username</label>
        <input asp-for="Username" />
    </div>
    <button type="submit">Register</button>
</form>